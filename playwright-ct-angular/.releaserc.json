{
  "branches": [ "main" ],
  "repositoryUrl": "git@github.com:sand4rt/playwright-ct-angular.git",
  "plugins": [
    "@semantic-release/release-notes-generator",
    [ "../semantic-release-version-mirror", { "dependency": "@playwright/experimental-ct-core" } ],
    [ "@semantic-release/exec", { "prepareCmd": "git ls-files -m -o" } ],
    [ "@semantic-release/git", {
      "assets": ["package.json"],
      "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
    }],
    [ "@semantic-release/npm", { "npmPublish": true, "tarballDir": "." } ],
    [ "@semantic-release/github", { "assets": "*.tgz" } ]
  ],
  "ci": true,
  "debug": true,
  "dryRun": false
}
